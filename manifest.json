{
  "name" : "Tabby",
  "manifest_version" : 2,
  "version" : "0.0.1",
  "description" : "Closes unattented tabs",
  "permissions" : ["tabs", "*://*/*"],
  "options_page" : "options.html",
  "content_scripts" : [ 
    {
      "matches" : ["*://*/*"],
      "js" : ["youtube.js"]
    }
  ],
  "background": {
    "scripts": ["tabby.js"]
  }
}
